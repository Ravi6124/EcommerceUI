uct/', this.productDTO).then(res => {\n          window.console.log('res datdadad', res.data)\n          // vm.pid = res.data\n          this.$store.commit('SET_PRODUCTINFO', {\n          imageURL: this.imageURL,\n          description: this.description,\n          pid: res.data\n        })\n        this.$router.push({name: 'merchantaddtolist', params: {pname: this.fromFirstPageGetter.product_name, pcategory: this.fromFirstPageGetter.select_category}});\n        });\n        window.console.log('pid', this.pid)\n        \n      }\n    }\n  }\n</script>\n\n<style scoped>\n  h1 {\n    text-align: center;\n  }\n  .main {\n    margin-left: 20%;\n  }\n  td {\n    padding-right: 20px;\n  }\n  input[type=\"text\"]{\n    border-radius: 3px;\n    width: 150%;\n  }\n  textarea {\n    border-radius: 3px;\n    width: 150%;\n  }\n  button {\n    margin-left: 29%;\n  }\n  .container {\n    margin-left: 13%;\n  }\n</style>"],"sourceRoot":"src/views"}]}